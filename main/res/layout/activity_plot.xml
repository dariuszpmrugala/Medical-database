<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.tamu.ecen.capstone.patientmd.activity.PlotActivity">

    <Spinner
        android:id="@+id/spinner_tests"
        android:layout_width="200dp"
        android:layout_height="40dp" />

    <Button
        android:id="@+id/button_date_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="LowDatePicker"
        android:text="Low Date"
        app:layout_constraintStart_toEndOf="@+id/spinner_tests" />

    <Button
        android:id="@+id/button_date_high"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="HighDatePicker"
        android:text="High Date"
        app:layout_constraintStart_toEndOf="@+id/button_date_low" />

    <Button
        android:id="@+id/button_plot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="160dp"
        android:layout_marginTop="16dp"
        android:onClick="Plot"
        android:text="Plot"
        app:layout_constraintStart_toStartOf="@+id/spinner_tests"
        app:layout_constraintTop_toBottomOf="@+id/line_chart" />

    <TextView
        android:id="@+id/textView_date_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:text="Low Date"
        app:layout_constraintStart_toStartOf="@+id/button_date_low"
        app:layout_constraintTop_toBottomOf="@+id/button_date_low" />

    <TextView
        android:id="@+id/textView_date_high"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:text="High Date"
        app:layout_constraintStart_toStartOf="@+id/button_date_high"
        app:layout_constraintTop_toBottomOf="@+id/button_date_high" />

    <com.jjoe64.graphview.GraphView
        android:id="@+id/line_chart"
        android:layout_width="370dp"
        android:layout_height="450dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_date_low"></com.jjoe64.graphview.GraphView>

</android.support.constraint.ConstraintLayout>
